{
  "name": "spotify-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start:test": "node build/index.js",
    "start2:test": "nodemon --exec npx ts-node ./index.ts",
    "build2:test": "tsc && node build/index.js",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "client": "cd client && npm start",
    "postinstall": "cd client && npm install"
  },
