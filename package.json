    "@types/express": "^4.17.13",
    "@types/node": "^16.7.10",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
